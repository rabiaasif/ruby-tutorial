<h1>Friends List</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Twitter</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      <% @friends.each do |friend| %>
        <%= render friend %>
      <% end %>
  </tbody>
</table>


<%= link_to "New friend", new_friend_path , class:"btn btn-primary" %>
